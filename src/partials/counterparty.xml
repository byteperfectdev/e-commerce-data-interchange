    <ОфициальноеНаименование>{{ company }}</ОфициальноеНаименование>
    <Имя>{{ first_name }}</Имя>
    <Фамилия>{{ last_name }}</Фамилия>
    <Контакты>
        <Контакт>
            <Тип>Почта</Тип>
            <Значение>{{ email }}</Значение>
        </Контакт>
        <Контакт>
            <Тип>ТелефонРабочий</Тип>
            <Значение>{{ phone }}</Значение>
        </Контакт>
    </Контакты>
</Контрагент>