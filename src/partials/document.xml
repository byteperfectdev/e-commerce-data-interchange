<Документ>
    <Номер>{{ order_id }}</Номер>
    <Дата>{{ date }}</Дата>
    <Время>{{ time }}</Время>
    <ХозОперация>Заказ товара</ХозОперация>
    <Роль>Продавец</Роль>
    <Валюта>{{ currency }}</Валюта>
    <Сумма>{{ total }}</Сумма>
    <Комментарий>{{ comment }}</Комментарий>
    <Контрагенты>
        {{ output_counterparties }}
    </Контрагенты>
    <Товары>
        {{ output_products }}
        {{ output_fees }}
        {{ output_shipping_methods }}
    </Товары>
    {{ output_attributes }}
</Документ>